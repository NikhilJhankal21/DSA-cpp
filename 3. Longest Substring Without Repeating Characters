class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        int n = s.length();
        int len = 0;
        int low = 0;
        unordered_map<char, int> mp;
        for (int high = 0; high < n; high++) {
            mp[s[high]]++;
            while (mp[s[high]] > 1) {
                mp[s[low]]--;
                low++;
            }
            len = max(len, high - low + 1);
        }
        return len;
    }
};
