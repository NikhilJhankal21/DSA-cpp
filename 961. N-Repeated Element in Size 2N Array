class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
        int n=nums.size();
        unordered_map<int,int>mp;
        for(auto x:nums){
            mp[x]++;
        }
        for(auto i:mp){
            if(i.second==n/2){
                return i.first;
            }
        }
        return 0;
    }
};
