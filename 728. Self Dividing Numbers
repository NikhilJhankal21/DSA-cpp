class Solution {
public:
    vector<int> selfDividingNumbers(int left, int right) {
        vector<int> ans;
        while (left <= right) {
            int k = left;
            bool flag = true;
            while (k) {
                int x = k % 10;
                k /= 10;
                if (x==0 || left % x != 0){
                    flag = false;
                    break;
                }
            }
            if(flag) ans.push_back(left);
            left++;
        }
        return ans;
    }
};
