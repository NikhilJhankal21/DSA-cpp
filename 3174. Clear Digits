class Solution {
public:
    string clearDigits(string s) {
        int n=s.length();
        string result="";
        for(int i=0;i<n;i++){
            if(isdigit(s[i])){
                result.pop_back();
            }
            else{
                result+=s[i];
            }
        }
        return result;
    }
};
