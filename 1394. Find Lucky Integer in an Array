class Solution {
public:
    int findLucky(vector<int>& arr) {
        int n=arr.size();
        int luckyint=-1;
        unordered_map<int,int>mp;
        for(int i=0;i<=n-1;i++){
            mp[arr[i]]++;
        }
        for(auto x:mp){
            int val=x.first;
            int freq=x.second;
            if(val==freq) luckyint= max(luckyint,val);
        }
        return luckyint;
    }
};
