class Solution {
public:
    bool doesAliceWin(string s) {
        for (char ch : s) {
            if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
                return true; // Alice can win
            }
        }
        return false; // No vowels, Alice loses
    }
};

// If there is atleast 1 vowel then alice will alwyas win. Bob can only win if there is no vowels.
// Unfair with vowel
