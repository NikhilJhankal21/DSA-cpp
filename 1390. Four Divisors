class Solution {
public:
    int sumFourDivisors(vector<int>& nums) {
        int totalSum = 0;
        for (int num : nums) {
            int count = 0, sum = 0;
            for (int i = 1; i * i <= num; ++i) {
                if (num % i == 0) {
                    int otherFact = num / i;
                    if (i == otherFact) {
                        count++;
                        sum += i;
                    } else {
                        count += 2;
                        sum += i + otherFact;
                    }
                }
                if (count > 4) break; // No need to continue if more than 4 divisors
            }
            if (count == 4) totalSum += sum;
        }
        return totalSum;
    }
};
