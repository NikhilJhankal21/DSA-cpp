class Solution {
public:
    int rearrangeCharacters(string s, string target) {
        unordered_map<char, int> mp;
        int ans = INT_MAX;
        for (int i = 0; i < s.length(); i++) {
            mp[s[i]]++;
        }
        unordered_map<char, int> mp2;
        for (int i = 0; i < target.size(); i++) {
            mp2[target[i]]++;
        }

        for (auto it : mp2) {
            char ch = it.first;
            int need = it.second;
            if (mp[ch] == 0)
                return 0;
            ans = min(ans, mp[ch] / need);
        }
        return ans;
    }
};
