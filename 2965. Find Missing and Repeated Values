class Solution {
public:
    vector<int> findMissingAndRepeatedValues(vector<vector<int>>& grid) {
        unordered_map<int,int>mp;
        // Pehle initialize krenge 0 se
        int repeating=0;
        int missing=0;
        int n=grid.size();
        for(int i=1; i<=n*n; i++){
            mp[i] = 0;
        }
        // fir freq increase krenge jo present h
        for(int i=0; i<grid.size(); i++){
            for(int j=0; j<grid[i].size(); j++){
                mp[grid[i][j]]++;
            }
        }
        // Jiski freq =0 missing h and freq=2 woh repeated 
        for(int i=1; i<=n*n; i++){
            if(mp[i]==0) missing = i;
            else if(mp[i]==2) repeating = i;
        }
        return {repeating, missing};
    }
};
