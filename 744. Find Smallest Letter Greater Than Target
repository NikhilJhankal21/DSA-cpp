class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
        int n = letters.size();
        for (int i = 0; i < n; i++) {
            if (letters[i] > target) {
                return letters[i];
            }
        }
        // Agar koi bada letter nahi mila toh pehla letter return karo
        return letters[0];
    }
};
