class Solution {
public:
    vector<string> buildArray(vector<int>& target, int n) {
        vector<string> ans;
        int m = target.size();
        int p=0;
        for (int i = 1; i <= n; i++) {
            if(i==target[p]){
                ans.push_back("Push");
                p++;
                if(p==m) break;
            }else{
                ans.push_back("Push");
                ans.push_back("Pop");
            }

        }
        return ans;
    }
};
